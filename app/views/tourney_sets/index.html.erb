<p id="notice"><%= notice %></p>

<h1>Tourney Sets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tourney_sets.each do |tourney_set| %>
      <tr>
        <td><%= link_to 'Show', tourney_set %></td>
        <td><%= link_to 'Edit', edit_tourney_set_path(tourney_set) %></td>
        <td><%= link_to 'Destroy', tourney_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tourney Set', new_tourney_set_path %>
